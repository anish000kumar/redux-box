{
  "name": "redux-box",
  "version": "1.6.2",
  "description": "A redux container to scaffold redux based applications in simplest possible way.",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest",
    "test-debug": "./node_modules/.bin/node-debug ./node_modules/.bin/jest -i --watch",
    "build": "babel src --out-dir build",
    "clean": "rimraf lib dist es coverage",
    "format": "prettier --write \"{src,test}/**/*.{js,ts}\" index.d.ts \"**/*.md\"",
    "format:check": "prettier --list-different \"{src,test}/**/*.{js,ts}\" index.d.ts \"**/*.md\"",
    "lint": "eslint src test",
    "pretest": "npm run build",
    "test:watch": "npm test -- --watch",
    "test:cov": "npm test -- --coverage",
    "prepare": "npm run clean && npm run format:check && npm run lint && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anish000kumar/redux-box.git"
  },
  "keywords": [
    "redux",
    "react",
    "javascript",
    "react-native",
    "vue"
  ],
  "author": "Anish Kumar",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/anish000kumar/redux-box/issues"
  },
  "dependencies": {
    "immer": "^0.8.4",
    "react": "^16.2.0",
    "react-redux": "5.0.6",
    "redux": "^3.7.2",
    "redux-saga": "0.16.0",
    "regenerator-runtime": "0.11.1",
    "reselect": "^3.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-jest": "^18.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "enzyme": "^2.7.0",
    "jest": "^18.1.0",
    "react-addons-test-utils": "15.2.0",
    "react-test-renderer": "^15.4.1",
    "redux-mock-store": "^1.5.1"
  },
  "homepage": "https://github.com/anish000kumar/redux-box#readme"
}
